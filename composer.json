{
    "name": "lambda-platform/laravel",
    "type": "project",
    "description": "Lambda platform",
    "homepage": "https://github.com/lambda-platform",
    "keywords": [
        "framework",
        "laravel",
        "lambda-platform"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kreait/laravel-firebase": "3.0.0",
        "lambda-platform/agent": "dev-dev",
        "lambda-platform/dataform": "dev-master",
        "lambda-platform/datagrid": "dev-dev",
        "lambda-platform/datasource": "dev-master",
        "lambda-platform/krud": "dev-dev",
        "lambda-platform/moqup": "dev-master",
        "lambda-platform/puzzle": "dev-master",
        "lambda-platform/template-paper": "dev-dev",
        "laravel/framework": "^8.12",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "^4.1.4"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/agent-laravel.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/dataform-php.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/datagrid-php.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/datasource-laravel.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/krud-laravel.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/moqup-laravel.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/puzzle-laravel.git"
        },
        {
            "type": "git",
            "url": "https://github.com/lambda-platform/template-laravel.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Helper\\": "Helper/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
